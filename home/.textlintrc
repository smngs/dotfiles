{
  "filters": {},
  "plugins": ["latex2e"],
  "rules": {
    "preset-ja-technical-writing": {
      "ja-no-mixed-period": {
        "periodMark": "．",
        "allowPeriodMarks": ["。"],
        "allowEmojiAtEnd": false,
        "forceAppendPeriod": false
      },
      "sentence-length": {
        "max": 300
      }
    }
  }
}
